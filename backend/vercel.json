{
  "version": 2,
  "builds": [
    {
      "src": "./",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./routes/Router.js"
    },
    {
      "src": "./uploads/(.*)",
      "dest": "./uploads/$1"
    },
    {
      "src": "./(.*)",
      "dest": "./app.js"
    }
  ]
}
