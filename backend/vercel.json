{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./routes/Router.js"
    },
    {
      "src": "./uploads/(.*)",
      "dest": "./uploads/$1"
    },
    {
      "src": "./(.*)",
      "dest": "./app.js"
    }
  ]
}
